{
  "name": "harmonica-tunning",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:soundslikeodd/harmonica-tunning.git",
  "author": "matthew schaadt <soundslikeodd@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "setup": "yarn install --pure-lockfile",
    "app": "yarn setup && yarn lint && webpack-dev-server --mode development --watch",
    "build": "yarn setup && webpack --mode production",
    "lint:es": "eslint --format=pretty --ext .jsx --ext .js .",
    "lint:style": "stylelint '**/*.scss'",
    "lint": "yarn lint:es && yarn lint:style"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxEOF": 1
        }
      ],
      "jsx-a11y/label-has-associated-control": [
        "error",
        {
          "controlComponents": [
            "Select"
          ]
        }
      ],
      "jsx-a11y/label-has-for": 0
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "history": "^4.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-select": "^3.0.4"
  }
}
